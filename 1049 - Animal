#include <iostream>
 #include<iostream>
using namespace std;
 
int main() {
    string a,b,c;
    
    cin>>a>>b>>c;
    if (a=="vertebrado" &&  b=="ave" )
    {
    if(c=="carnivoro"){
    cout<<"aguia"<<endl;
    }
    else if(c=="onivoro"){
      cout<<"pomba"<<endl;  
    }
  }
  if(a=="vertebrado" && b=="mamifero" ){
    if(c=="onivoro"){
    cout<<"homem"<<endl;
    }
    else if(c=="herbivoro"){
      cout<<"vaca"<<endl;  
    }
  }
  
  if(a=="invertebrado" && b=="inseto" ){
    if(c=="hematofago"){
    cout<<"pulga"<<endl;
    }
    else if(c=="herbivoro"){
      cout<<"lagarta"<<endl;  
    }
  }
   if(a=="invertebrado" && b=="anelideo" ){
    if(c=="hematofago"){
    cout<<"sanguessuga"<<endl;
    }
    else if(c=="onivoro"){
      cout<<"minhoca"<<endl;  
    }
  }
  

    
    return 0;
}
