#include <iostream>
 
using namespace std;
 
int main() {
 
    int n,a,i,cp=0,cb=0;
    cin>>n;
    for(i=1;i<=n;i++){
        cin>>a;
        if(a>=10 && a<=20){
            cp++;
        }
        else{
            cb++;
        }
    }
    cout<<cp<<" in"<<endl;
    cout<<cb<<" out"<<endl;
    return 0;
}
